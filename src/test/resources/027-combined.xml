<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="#connectSchema">
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" id="connectSchema">
        <xs:element name="root">
            <xs:complexType>
                <xs:sequence>
                    <xs:any maxOccurs="1" minOccurs="0" namespace="http://www.w3.org/2001/XMLSchema" processContents="skip" />
                    <xs:element name="mystring" type="xs:string" />
                    <xs:element name="onechar" type="xs:string" />
                    <xs:element name="mybytes" type="xs:base64Binary" />
                    <xs:element name="onebyte" type="xs:base64Binary" />
                </xs:sequence>
            </xs:complexType>
        </xs:element>
    </xs:schema>

    <mystring>Hello World</mystring>
    <onechar>H</onechar>
    <mybytes>SGVsbG8gV29ybGQ=</mybytes>
    <onebyte>SA==</onebyte>
</root>